#lang racket
(require csc151)
(require 2htdp/image)
(define cos-func
  (lambda (height)
    (let ([half-func (add-curve (rectangle 300 300 "solid" "white")
                                0 200 0 0.3333
                                300 0 0 0.3333
                                "red")])
      (scale (/ height 300)
             (beside
              half-func
              (flip-horizontal half-func)
              half-func
              (flip-horizontal half-func))))))
